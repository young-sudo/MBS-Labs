	note = {Publisher: Nature Publishing Group},
	keywords = {Genomics, Epigenomics},
	pages = {293--304},
	file = {Full Text PDF:C\:\\Users\\pabie\\Zotero\\storage\\4NTQ5XDL\\Hao et al. - 2024 - Dictionary learning for integrative, multimodal an.pdf:application/pdf},
